$title = 'Registered Users';
$this->headTitle($title);
?>
<h1><?php echo $this->escapeHtml($title); ?></h1>


<table class="table">
<tr>

    <th>First  Name</th>
    <th>Family Name</th>
    <th>Nick Name</th>
   
</tr>
<?php foreach ($users as $user) : ?>
